{
  "name": "firefox-remix-theme",
  "displayName": "Firefox Theme",
  "description": "A remix of the Firefox Theme for vscode. Personal theme",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyleculp/firefox-remix"
  },
  "engines": {
    "vscode": "^1.41.1"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Firefox Remix Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      },
      {
        "label": "Firefox Remix Light",
        "uiTheme": "vs",
        "path": "./themes/light.json"
      }
    ]
  },
  "dependencies": {
    "jsonc": "^2.0.0",
    "strip-json-comments": "^3.0.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  }
}
